# PostgreSQL connection settings for SSB benchmark
DB_HOST=127.0.0.1
DB_PORT=5432
DB_USER=app_user
DB_PASSWORD=secure_password
DB_NAME=ssb_db

# Cache Backend (for PostgreSQL mode)
# Options: postgresql_array, postgresql_bit, postgresql_roaringbit
CACHE_BACKEND=postgresql_array

# DuckDB Bit Cache settings (for DuckDB mode)
# Date keys are YYYYMMDD integers, so bitsize must be > 20M
DUCKDB_BIT_PATH=ssb_cache.duckdb
DUCKDB_BIT_BITSIZE=20000000
