# PostgreSQL connection settings for TPC-H benchmark
DB_HOST=127.0.0.1
DB_PORT=5432
DB_USER=app_user
DB_PASSWORD=secure_password
DB_NAME=tpch_db

# Cache Backend (for PostgreSQL mode)
# Options: postgresql_array, postgresql_bit, postgresql_roaringbit
CACHE_BACKEND=postgresql_array

# DuckDB Bit Cache settings (for DuckDB mode)
# Order keys are sequential integers; bitsize scales with SF
DUCKDB_BIT_PATH=tpch_cache.duckdb
DUCKDB_BIT_BITSIZE=200000
